<date_of_birth>
<customDisplay>{{velocity}}{{html wiki=false clean=false}}
#set ($definedFormat = "$!obj.xWikiClass.get($name).getProperty('dateFormat').value")
#set($config = $xwiki.getDocument("${doc.space}.WebHome").getObject('ClinicalInformationCode.DBConfigurationClass'))
#if ($config)
  #set ($format = "$!{config.getProperty('dateOfBirthFormat').value}")
#end
#if ("$!format" == '')
  #set ($format = $definedFormat)
#end
#if ($xcontext.action == 'edit')
  #if ($format.indexOf('d') &lt; 0)
    &lt;span class="$name" id="$prefix${name}_container"&gt;
      &lt;input type="hidden" id="$prefix$name" name="$prefix$name" value="$xwiki.formatDate($value, $definedFormat)"/&gt;
      #set ($date = $util.date)
      #if ($value)
        #set ($selMonth = $value.month)
        #set ($selYear= $value.year)
      #else
        #set ($selMonth = $date.month)
        #set ($selYear= $date.year)
      #end
      #set ($mFormat = $format.replaceAll("[^M]", ''))
      #set ($yFormat = $format.replaceAll("[^y]", ''))

      &lt;select name="$prefix${name}_month"&gt;
      #foreach ($val in [0..11])
        #set ($date.month = $val)
        &lt;option value="$val"#if ($selMonth == $val) selected="selected"#end&gt;$xwiki.formatDate($date, $mFormat)&lt;/option&gt;
      #end
      &lt;/select&gt;

      #set ($startYear = $date.year - 99)
      #set ($endYear = $date.year + 1)

      &lt;select name="$prefix${name}_year"&gt;
      #foreach ($val in [$startYear..$endYear])
        #set ($date.year = $val)
        &lt;option value="$val"#if ($selYear == $val) selected="selected"#end&gt;$xwiki.formatDate($date, $yFormat)&lt;/option&gt;
      #end      
      &lt;/select&gt;
    &lt;/span&gt;
  #else
    &lt;input type="text" class="date" title="${format}" id="$prefix$name" name="$prefix$name" value="$xwiki.formatDate($value, $definedFormat)"/&gt;
  #end
#else
&lt;span class="date"&gt;$xwiki.formatDate($value, $format)&lt;/span&gt;
#end
{{/html}}{{/velocity}}</customDisplay>
