#body {
    font-family: Arial,Verdana,sans-serif;
}
#hierarchy,
#globallinks,
.edit-meta-tools,
#contentmenu,
#document-info,
.edit_section {
  display: none !important;
}
#headerglobal {
  position: relative;
}
#company h1 a {
  color: $theme.pageBackgroundColor !important;
  text-decoration: none !important;
}
#company h1 {
  border: 0 none;
  xbottom: 20px;
  color: $theme.pageBackgroundColor;
  margin: 0 20px 0;
  position: absolute;
  right: 5%;
  text-shadow: 0 0 6px #777;
  text-transform: uppercase;
  font-size: 1.2em;
}
#company .sep {
  fdisplay: none;
}
#headerglobal {
  border-bottom: 2px solid;
}
.xdocCreation {
  float: right;
  width: 48%;
}
#document-title h1 {
    margin: 0 0 0.5em;
    padding: 5px 0 0;
}
#mainContentArea, #mainEditArea {
    ##padding: 10px 10%;
    margin: 4px 10%;
    padding: 10px;
  overflow: visible;
}
.actionmenu .tmHome {
    background-image: url("#__img('home-s')");
}
.actionmenu .tmAdminSpace {
    background-image: url("#__img('laptop-s')");
}
.actionmenu .tmUser {
    background-image: url("#__img('user-s')");
}

input.button, .buttonwrapper a {
  #__css3_rule("border-radius: 4px")
  #__css3_rule("box-shadow: 0 0 6px $theme.textColor");
}
input.button:hover, .buttonwrapper a:hover {
  background-color: $theme.buttonPrimaryBackgroundColor;
  color: $theme.buttonPrimaryTextColor;
  #__css3_rule("box-shadow: 0 0 6px $theme.linkColor")
}

input[name=action_saveandcontinue] {
  display: none;
}
.bottombuttons input[type=submit] {
  padding-left: 20px;
  background-position: 4px 40%;
  background-repeat: no-repeat;
}
input[name=action_save] {
  background-image: url("#__img('check-s')") !important;
}
input[name=action_cancel] {
  background-image: url("#__img('delete-s')") !important;
}

.add-data-button,
.buttonwrapper a.add-data-button:link,
.buttonwrapper a.add-data-button:visited,
.buttonwrapper a.add-data-button:active {
  #__css3_rule("border-radius: 4px")
  #__css3_rule("box-shadow: 0 0 6px $theme.borderColor")
  background-color: $theme.buttonPrimaryBackgroundColor;
  color: $theme.buttonPrimaryTextColor;
  display: inline-block;
  font: normal normal 700 .8em/140% "Helvetica",sans-serif;
  padding: .3em .2em .3em  2.2em;
  position: relative;
  text-transform: uppercase;
  white-space: nowrap;
}
.add-data-button:hover,
.buttonwrapper a.add-data-button:hover {
  #__css3_rule("box-shadow: 0 0 6px $theme.linkColor")
}
.general-tools {
  margin: 25px 3px;
  padding: 3px 0;
  float: right;
  position: relative;
}
.general-tools label{
  cursor: pointer;
  color: $theme.linkColor;
}
.general-tools label:hover {
  text-decoration: underline;
}
#create-patient-record {
  padding: .3em .2em .3em  2.2em;
}
#patient-creation-form {
  position: relative;
}
.xwiki-livetable-pagination #patient-creation-form {
  float: left;
}
label.create-button-label, .add-data-button:before {
  display: inline-block;
  position: absolute;
  top: .24em;
  left: .4em;
  width: .8em;
  height: .8em;
  color: $theme.buttonPrimaryTextColor;
  text-align: center;
  font-weight: bold;
  cursor: pointer;
  font-size: 1.25em;
  line-height: .9em;
  vertical-align: middle;
  z-index: 2;
}
body .xwiki-livetable-topfilters-container, body .xwiki-livetable-tagcloud-container {
  border-color: $theme.buttonPrimaryBackgroundColor;
  background-color: $theme.backgroundSecondaryColor;
}
body .xwiki-livetable-topfilters-tip .tippointer, body .xwiki-livetable-tagcloud-tip .tippointer {
  border-top-color: $theme.buttonPrimaryBackgroundColor;
}
body .xwiki-livetable-topfilters-tip .tippointer div, body .xwiki-livetable-tagcloud-tip .tippointer div {
  border-top-color: $theme.backgroundSecondaryColor;
  top: -22px;
}
body .xwiki-livetable-topfilters-container .value-cloud-tools a {
  color: $theme.textColor;
  font-weight: bold;
}
#patients.xwiki-livetable .actions {
  text-align: right;
}
.document-action, #table-of-contents, #patients.xwiki-livetable .action, .action {
  #__css3_rule("border-radius: 6px")
  #__css3_rule("box-shadow: 0 0 6px $theme.borderColor")
  text-align: left;
}
#document-actions {
  color: $theme.textColor;
  position: fixed;
  right: 4px;
  top: 8.5em;
  margin-right: -8px;
  right: 5%;
}
#document-actions.absolute {
  position: absolute;
  top: 0;
  right: 0;
  float: right;
  margin-top: -5px;
  margin-right: -32px;
  height: 0;
  overflow: visible;
}
* html #document-actions {
  position: absolute;
}
*+html #document-actions {
  position: absolute;
}
.document-action, #patients.xwiki-livetable .action, .action {
  display: block;
  text-indent: -9999px;
  width: 22px;
  height: 26px;
  background: none $theme.buttonPrimaryBackgroundColor 3px 5px no-repeat;
  margin: 4px 0;
  #__css3_rule("box-shadow: 0 0 5px rgba(0, 0, 0, 0.25), 0 0 10px rgba(0, 0, 0, 0.1) inset")
  border: 1px solid $theme.buttonPrimaryBackgroundColor !important;
}
#patients.xwiki-livetable .action {
  display: inline-block;
  padding: 0;
}
.document-action-button {
  width: 24px !important;
}
.document-action:hover, #patients.xwiki-livetable .action:hover, .action:hover {
  box-shadow: 0 0 6px $theme.linkColor;
  cursor: pointer;
}
#tmActionEdit, #patients.xwiki-livetable .actionedit {
    background-image: url("#__img('editb-s')");
}
#tmExportPdf {
  background-image: url("$xwiki.getSkinFile('icons/silk/page_white_acrobat.png')");
}
#tmHistory {
  background-image: url("$xwiki.getSkinFile('icons/silk/page_white_stack.png')");
  margin-top: -30px;
}
#tmActionDelete, #patients.xwiki-livetable .actiondelete, .action.delete {
    background-image: url("#__img('no-s')");
}
#tmActionsave {
    background-image: url("#__img('check-s')");
}
#tmActioncancel {
    background-image: url("#__img('delete-s')");
}
#tmToC, #tmHideToC, #patients.xwiki-livetable .actionview {
    background-image: url("#__img('toc-s')");
}
#table-of-contents {
  background-color: $theme.highlightColor;
  border: 1px solid $theme.borderColor;
  position: fixed;
  top: 9em;
  right: 5%;
  padding: 1em 1em 1em 0;
  ##display:none;
  visibility: hidden;
  z-index: 20000;
  width: 22%;
  margin-top: -2px;
  margin-right: 20px;
}
#table-of-contents.absolute {
  position: absolute;
  margin-right: -4px;
  margin-left: -16px;
  margin-top: 0;
  top: 0;
  right: 0;
}
#table-of-contents ul {
  padding: 0;
  margin: 0 0 0 1em;
  list-style-type: square;
}
#table-of-contents:target {
  ##display: block;
  visibility: visible;
}
#hide-toc {
  position: absolute;
  right: 0.25em;
  top: -0.5em;
}
.hide-toc p {
  padding 0;
  margin 0;
  line-height: 1em;
}
#hide-toc a {
  padding: 0 0.25em;
  text-decoration: none !important;
  color: $theme.textColor !important;
}
#table-of-contents .hide-toc-ghost {
  margin-top: -2px;
  position: absolute;
  right: -29px;
  top: -0.5em;
}
h1:target, h2:target, h3:target {
  background-color: $theme.highlightColor;
}
#globallinks {display: none;}

## Disable gradients
#mainmenu {
  background-image: none;
  box-shadow: none;
}
input.button, input.button:hover, .buttonwrapper a, .buttonwrapper a:link, .buttonwrapper a:active, .buttonwrapper a:hover {
  background-image: none;
}
#mainmenu * {
  text-shadow: none;
